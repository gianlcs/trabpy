idade = int(input("Digite sua idade: "))
nacionalidade = input("Digite sua nacionalidade: ").lower()

if idade >= 18:
    if nacionalidade == "brasileira" or nacionalidade == "brasileiro":
        print("Voto obrigatório")
    else:
        print("Voto opcional (estrangeiro)")
elif idade >= 16:
    print("Voto opcional")
else:
    print("Não pode votar")
